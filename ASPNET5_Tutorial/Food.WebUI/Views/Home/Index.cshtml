@model IEnumerable<Tutorial.Data.Models.Restaurant>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Welcome to Ode To Food!</h1>
    <div>
        There are @Model.Count() restaurants in the database.
    </div>
</div>

<ul>
    @foreach(var r in Model)
    {
        <li>@r.Name</li>
    }
</ul>